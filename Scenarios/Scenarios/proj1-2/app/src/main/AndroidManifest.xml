<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Proj1"
        tools:targetApi="31">
        <activity
            android:name=".AnswerOwnQuestions"
            android:exported="false" />
        <activity
            android:name=".createYourOwnQuestion"
            android:exported="false" />
        <activity
            android:name=".hardModeQuestions"
            android:exported="false" />
        <activity
            android:name=".mediumModeQuestions"
            android:exported="false" />
        <activity
            android:name=".easyModeQuestions"
            android:exported="false" />
        <activity
            android:name=".Home"
            android:exported="false" />
        <activity
            android:name=".questionStyle"
            android:exported="false"
            android:label="@string/title_activity_question_style"
            android:theme="@style/Theme.Proj1.NoActionBar" />
        <activity
            android:name=".register"
            android:exported="false">
            <intent-filter>
                <category android:name="android.intent.category.SECONDARY_HOME" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>